{
  "name": "teunrutten/hubspot-boilerplate",
  "version": "0.0.1",
  "description": "Local Hubspot development",
  "main": "index.js",
  "author": "Teun Rutten & Linsey Brander",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rm -rf dist && mkdir dist ",
    "build": "yarn clean:dist && yarn build:gulp && yarn build:webpack",
    "build:webpack": "webpack",
    "build:gulp": "gulp build",
    "build:icons": "gulp build-icons",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "watch": "gulp watch",
    "watch:webpack": "webpack --watch --progress",
    "release": "standard-version && git push --follow-tags && git push && git push --tags",
    "gen:fields": "gulp generate-fields",
    "gen:module": "yarn plop; yarn gen:fields",
    "hs:auth": "hscms auth oauth2",
    "hs:sync-staging": "yarn gen:fields; yarn build:icons; hscms upload --portal=staging dist public",
    "hs:sync-prod": "yarn gen:fields; yarn build:icons; hscms upload --portal=prod dist public",
    "hs:sync-dev": "yarn gen:fields; yarn build:icons; hscms upload --portal=dev dist public",
    "hs:module": "hscms create module",
    "hs:watch": "hscms watch --portal=dev dist public",
    "deploy:staging": "yarn build && yarn hs:sync-staging",
    "deploy:prod": "yarn build && yarn hs:sync-prod",
    "deploy:dev": "yarn build && yarn hs:sync-dev"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@hubspot/cms-cli": "^1.0.7",
    "autoprefixer": "9.7.3",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "del": "^5.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^12.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-changed": "^4.0.2",
    "gulp-cheerio": "^0.6.3",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-ext-replace": "^0.3.0",
    "gulp-footer": "^2.0.2",
    "gulp-hash": "^4.2.2",
    "gulp-header": "^2.0.9",
    "gulp-load-plugins": "^2.0.0",
    "gulp-memory-cache": "^0.3.0",
    "gulp-merge-json": "^2.1.1",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob-import": "^0.1.0",
    "gulp-svgmin": "^2.2.0",
    "gulp-svgstore": "^7.0.1",
    "json-merger": "^1.1.1",
    "node-sass": "^4.12.0",
    "plop": "^2.5.3",
    "postcss": "^7.0.21",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.0",
    "require-dir": "^1.2.0",
    "standard-version": "^7.0.0",
    "through2": "^3.0.1",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "body-scroll-lock": "^3.0.2",
    "object-fit-images": "^3.2.4",
    "objectFitPolyfill": "^2.3.0",
    "preact": "^10.1.1",
    "raven-js": "^3.27.2",
    "smoothscroll": "^0.4.0"
  }
}
